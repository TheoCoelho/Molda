{
  "name": "molda-optimization-agent",
  "version": "1.0.0",
  "description": "Performance optimization agent for Molda project",
  "type": "module",
  "scripts": {
    "optimize:full": "node src/orchestrator.js",
    "analyze:bundle": "node src/analyzers/bundleAnalyzer.js",
    "analyze:components": "node src/analyzers/componentAnalyzer.js",
    "analyze:performance": "node src/analyzers/performanceAnalyzer.js",
    "optimize:images": "node src/optimizers/imageOptimizer.js",
    "optimize:splitting": "node src/optimizers/codeSplitting.js",
    "optimize:vite": "node src/optimizers/viteOptimizer.js",
    "watch:performance": "node src/watchers/performanceWatcher.js",
    "report:generate": "node src/reporters/reportGenerator.js",
    "report:dashboard": "node src/reporters/dashboardGenerator.js",
    "validate:changes": "node src/validators/changeValidator.js",
    "rollback:changes": "node src/utils/rollback.js",
    "dev": "node src/dev.js"
